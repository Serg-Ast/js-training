{
  "name": "@we-learn-js/js-training",
  "version": "0.0.0",
  "description": "JS Training",
  "main": "index.js",
  "scripts": {
    "reset": "rm -Rf node_modules && npm i",
    "images": "node scripts/list-images.js",
    "build":
      "npm run images && parcel build --no-source-maps -d ./build src/index.html && cp ./scripts/fixtures/build/package.json ./build/package.json",
    "clear": "rm -Rf .cache dist build",
    "start":
      "npm run images && parcel serve --no-source-maps -p 3000 src/index.html",
    "dev": "npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "co": "sui-mono commit",
    "deploy": "now --public --name=js-training-v2 ./build",
    "deploy:clear": "now rm js-training-v2 --yes",
    "deploy:alias": "now alias set $(pbpaste) js-training-v2",
    "build-deploy":
      "npm run wiki:push && npm run build && npm run deploy:clear && npm run deploy && npm run deploy:alias",
    "wiki:add":
      "git subtree add --prefix=src/md git@github.com:we-learn-js/js-training.wiki.git master --squash",
    "wiki:pull":
      "git subtree pull --prefix=src/md git@github.com:we-learn-js/js-training.wiki.git master --squash -m \"Merge changes from origin repository.\"",
    "wiki:push":
      "git subtree push --prefix=src/md git@github.com:we-learn-js/js-training.wiki.git master"
  },
  "dependencies": {
    "@material-ui/icons": "1.0.0-beta.42",
    "fast-deep-equal": "1.1.0",
    "firebase": "4.12.1",
    "material-ui": "1.0.0-beta.42",
    "param-case": "2.1.1",
    "prismjs": "1.14.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-loadable": "5.3.1",
    "react-router": "4.2",
    "react-router-dom": "4.2",
    "reveal.js": "3.6.0",
    "three": "0.70.0"
  },
  "devDependencies": {
    "@s-ui/mono": "1",
    "babel-preset-sui": "1.5.0",
    "caniuse-lite": "1.0",
    "fs-extra": "5.0.0",
    "node-sass": "4.8.3",
    "node-sass-package-importer": "5.1.1",
    "now": "11",
    "parcel-bundler": "1.7.1",
    "postcss-cssnext": "3.1.0",
    "postcss-modules": "1.1.0"
  },
  "config": {
    "sui-mono": {
      "access": "public",
      "packagesFolder": "./src"
    }
  },
  "author": "David Almeida <davidbarna@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/we-learn-js/js-training.git"
  },
  "bugs": {
    "url": "https://github.com/we-learn-js/js-training/issues"
  },
  "homepage": "https://github.com/we-learn-js/js-training#readme"
}
