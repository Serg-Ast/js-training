{
  "name": "@davidbarna/js-training",
  "version": "0.0.0",
  "description": "JS Training",
  "main": "index.js",
  "scripts": {
    "start": "reveal-md --theme js-training ./src/slides/",
    "dev": "reveal-md ./src/slides/ --theme js-training --disableAutoOpen",
    "co": "sui-mono commit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "rm -Rf node_modules && npm i"
  },
  "dependencies": {
    "reveal-md": "0.4"
  },
  "devDependencies": {
    "@s-ui/mono": "1"
  },
  "config": {
    "sui-mono": {
      "access": "public",
      "packagesFolder": "./src"
    }
  },
  "author": "David Almeida <davidbarna@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbarna/js-training.git"
  },
  "bugs": {
    "url": "https://github.com/davidbarna/js-training/issues"
  },
  "homepage": "https://github.com/davidbarna/js-training#readme"
