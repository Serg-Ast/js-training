{
  "name": "@davidbarna/js-training",
  "version": "0.0.0",
  "description": "JS Training",
  "main": "index.js",
  "scripts": {
    "start": "reveal-md src/slideshow",
    "dev": "reveal-md src/slideshow -w --disable-auto-open",
    "sass": "node-sass --source-map true --importer node_modules/node-sass-package-importer/dist/cli.js -o src/slideshow/theme src/slideshow/theme/js-training.scss",
    "co": "sui-mono commit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "rm -Rf node_modules && npm i"
  },
  "dependencies": {
    "reveal-md": "0.4"
  },
  "devDependencies": {
    "@s-ui/mono": "1",
    "node-sass": "^4.5.3",
    "node-sass-package-importer": "^5.0.0-alpha.14"
  },
  "config": {
    "sui-mono": {
      "access": "public",
      "packagesFolder": "./src"
    }
  },
  "author": "David Almeida <davidbarna@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbarna/js-training.git"
  },
  "bugs": {
    "url": "https://github.com/davidbarna/js-training/issues"
  },
  "homepage": "https://github.com/davidbarna/js-training#readme"
}
